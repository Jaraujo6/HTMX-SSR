    "start": "node ./dist/functions/server.js"
  },
  "dependencies": {
    "@fastify/aws-lambda": "4.1.0",
    "@fastify/static": "7.0.3",
    "@fastify/view": "9.0.0",
    "@netlify/functions": "2.7.0",
    "fastify": "4.26.2",
    "handlebars": "4.7.8",
    "typescript": "5.4.3"
  },
  "devDependencies": {
    "@types/node": "20.12.5",
    "tsx": "4.7.2"
  }
}
